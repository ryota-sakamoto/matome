GET     /                           controllers.HomeController.index
GET     /settings/blogs             controllers.SettingController.blogList
GET     /settings/blogs/new         controllers.SettingController.blogCreate
POST    /settings/blogs/new         controllers.SettingController.blogInsert
GET     /settings/blogs/:id         controllers.SettingController.blogEdit(id: String)
POST    /settings/blogs/:id         controllers.SettingController.blogUpdate(id: String)
GET     /login                      controllers.LoginController.index
POST    /login                      controllers.LoginController.login
GET     /logout                     controllers.LoginController.logout
POST    /register                   controllers.RegisterController.register
GET     /$id<[0-9a-zA-Z_]+>         controllers.UserController.show(id: String)
GET     /$id<[0-9a-zA-Z_]+>/rss     controllers.UserController.rss(id: String)

GET     /api/v1/blogs/:id/articles  controllers.ApiController.getArticle(id: String)
POST    /api/v1/aggregate           controllers.CronController.aggregateArticle

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
